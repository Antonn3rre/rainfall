char *c;

void m()
{
    printf("%s - %d\n", &c, time(0));
    return
}

int main(int argc, char **argv) {

  char *str_1;
  char *str_2;
  FILE *pass;

  str_1 = malloc(8);
  str_1[0] = 1;
  str_1[4] = malloc(8);

  str_2 = malloc(8);
  str_2[0] = 2;
  str_2[4] = malloc(8);

  strcpy(str_1[4], argv[1]);
  strcpy(str_2[4], argv[2]);

  pass = fopen("/home/user/level8/.pass","r");
  fgets(c, 68, pass);
  puts("~~");
  return 0;
}